<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.leibnix.admin.rcp.ui.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="org.leibnix.admin.rcp.ui.Perspective"
            id="org.leibnix.admin.rcp.ui.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="View"
            class="org.leibnix.admin.rcp.ui.View"
            id="org.leibnix.admin.rcp.ui.view">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.elementFactories">
      <factory
            class="org.leibnix.admin.rcp.ui.NavigatorRoot"
            id="org.leibnix.admin.rcp.ui.NavigatorRoot">
      </factory>
   </extension>

</plugin>
